    "import string\n",
    " \n",
    "ipt = \"Python is great. Python is powerful, and Python is easy to learn!\"\n",
    "\n",
    "split_word = ipt.split()\n",
    "arr = set()\n",
    "\n",
    "for i in split_word:\n",
    "    remove_punctuation = i.strip(string.punctuation).lower()\n",
    "    arr.add(remove_punctuation)\n",
    "\n",
    "print(len(arr))"
   ]
  }
