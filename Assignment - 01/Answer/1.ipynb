   "source": [
    "N = int(input(\"Enter the number: \"))\n",
    "\n",
    "if N % 2 == 0:\n",
    "    print(\"EVEN\")\n",
    "else:\n",
    "    print(\"ODD\")\n",
    "\n",
    "if N < 0:\n",
    "    print(\"NEGATIVE\")   \n",
    "elif N > 0:\n",
    "    print(\"POSITIVE\")\n",
    "elif N == 0:\n",
    "    print(\"ZERO\")\n"
   ]
  }
